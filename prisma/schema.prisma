generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id            String   @id
  name          String
  email         String   @unique
  password      String
  registered_at DateTime @default(now())
  isAdmin       Boolean  @default(false)

  @@map("user")
}
